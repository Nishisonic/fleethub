{
  "name": "fleethub",
  "version": "1.7.13",
  "private": true,
  "license": "UNLICENSED",
  "author": "madono (https://github.com/MadonoHaru) <madonoharu@gmail.com>",
  "workspaces": [
    "packages/*",
    "crates/fleethub-core"
  ],
  "scripts": {
    "build": "yarn build:site",
    "build:admin": "yarn workspace @fh/admin build",
    "build:core": "bash scripts/build-core.sh",
    "build:kcs": "yarn workspace kcs build",
    "build:site": "yarn workspace @fh/site build",
    "build:utils": "yarn workspace @fh/utils build",
    "dev": "yarn workspace @fh/site dev",
    "dispatch": "yarn ts scripts/dispatch",
    "postinstall": "husky install",
    "lint": "eslint --cache packages && yarn tsc --incremental --noEmit",
    "site": "yarn workspace @fh/site",
    "start": "yarn workspace @fh/site start",
    "test": "jest",
    "test:fleethub-macro": "wasm-pack test --node crates/fleethub-macro",
    "ts": "NODE_OPTIONS='--experimental-specifier-resolution=node' ts-node-esm",
    "update:locales": "yarn ts scripts/updateLocales",
    "update:storage": "yarn ts scripts/updateStorage",
    "update:types": "yarn ts scripts/updateTypes",
    "utils": "yarn workspace @fh/utils"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": "prettier --cache --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --ext .ts,.tsx --fix"
  },
  "devDependencies": {
    "@fh/admin": "workspace:*",
    "@fh/utils": "workspace:*",
    "@next/eslint-plugin-next": "^12.2.2",
    "@swc/core": "^1.2.213",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.0.3",
    "@types/signale": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fleethub-core": "npm:latest",
    "fs-extra": "^10.1.0",
    "got": "12.1.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "kc-tools": "^1.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "signale": "^1.4.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "npm": "Please use yarn"
  }
}
