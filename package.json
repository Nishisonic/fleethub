{
  "name": "fleethub",
  "version": "1.3.3",
  "private": true,
  "license": "UNLICENSED",
  "author": "madono (https://github.com/MadonoHaru) <madonoharu@gmail.com>",
  "workspaces": [
    "packages/*",
    "crates/fleethub-core"
  ],
  "scripts": {
    "build": "yarn up fleethub-core@npm:latest equipment-bonus && yarn build:site",
    "build:admin": "yarn workspace @fh/admin build",
    "build:core": "bash scripts/build-core.sh",
    "build:kcs": "yarn workspace kcs build",
    "build:site": "yarn workspace @fh/site build",
    "build:utils": "yarn workspace @fh/utils build",
    "dev": "yarn workspace @fh/site dev",
    "dispatch": "yarn ts scripts/dispatch",
    "postinstall": "husky install && yarn build:utils && yarn build:admin && yarn build:kcs",
    "lint": "eslint --cache packages && yarn tsc --incremental --noEmit",
    "site": "yarn workspace @fh/site",
    "start": "yarn workspace @fh/site start",
    "test": "jest",
    "test:fleethub-macro": "wasm-pack test --node crates/fleethub-macro",
    "ts": "ts-node",
    "update:locales": "yarn ts scripts/updateLocales",
    "update:storage": "yarn ts scripts/updateStorage",
    "update:types": "yarn ts scripts/updateTypes",
    "utils": "yarn workspace @fh/utils"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --ext .ts,.tsx --fix"
  },
  "devDependencies": {
    "@fh/admin": "workspace:packages/admin",
    "@fh/utils": "workspace:packages/utils",
    "@next/eslint-plugin-next": "^12.1.5",
    "@swc/core": "^1.2.172",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "@types/signale": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "fleethub-core": "npm:latest",
    "fs-extra": "^10.1.0",
    "got": "11.8.3",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.0.2",
    "jest-environment-jsdom": "^28.0.2",
    "kc-tools": "^1.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "signale": "^1.4.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "npm": "Please use yarn"
  },
  "packageManager": "yarn@3.2.0"
}
