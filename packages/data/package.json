{
  "name": "@fleethub/data",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "author": "madono (https://github.com/MadonoHaru)",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "lib",
    "esm",
    "json"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p ./tsconfig.build.json -m commonjs --outDir lib",
    "build:esm": "tsc -p ./tsconfig.build.json -m esnext --outDir esm",
    "lint": "eslint --fix src/* scripts/* && prettier --write json/*.json",
    "tsnode": "yarn ts-node -O \"{\"\"module\"\":\"\"commonjs\"\", \"\"types\"\":[\"\"node\"\"]}\"",
    "update": "yarn tsnode scripts/update && yarn lint",
    "update:maps": "yarn tsnode scripts/maps && prettier --write json/maps.json"
  }
}
